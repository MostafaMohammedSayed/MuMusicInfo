<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymusicinfo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMusicInfo">
        <activity
            android:name=".ClassicActivity"
            android:label="@string/classic_text">
        </activity>
        <activity android:name=".CountryActivity"
            android:label="@string/country_text"/>
        <activity android:name=".BluesActivity"
            android:label="@string/blues_text"/>
        <activity
            android:name=".JazzActivity"
            android:label="@string/jazz_text" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>